<!-- * Initialize Default Values-->
<script>
  headerJS.addEventListeners();
  calendarJS.currentDate = calendarJS.getCurrentDate();
  calendarJS.activeDate = JSON.parse(JSON.stringify(calendarJS.currentDate));
  calendarMonthJS.pages = calendarJS.getMonthPages(calendarJS.minYear, calendarJS.maxYear);
  let entryDB = <%-entryDBProxy%>;
  addEntriesToCalendarPages(entryDB);
  calendarWeekJS.pages = calendarJS.getWeekPages(calendarMonthJS.pages);
  // let mode = 
  // console.log(`WHAT`); 
  // console.log(<%mode%>); 
  // console.log('<%-mode%>'); 
  headerJS.changeViewMode('<%-mode%>');
</script>

<!-- * Bootstrap Tooltip Initialize -->
<script type="text/javascript">
  var tooltips = document.querySelectorAll('.btn-tt')
  tooltips.forEach(t => {
    new bootstrap.Tooltip(t)
  })
</script>